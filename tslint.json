{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "semicolon": [true, "never"],
    "max-line-length": [true, 80],
    "trailing-comma": [false],
    "quotemark": [true, "single"],
    "no-console": false,
    "object-literal-sort-keys": false,
    "interface-name": false,
    "member-access": false,
    "ordered-imports": false
  },
  "rulesDirectory": []
}